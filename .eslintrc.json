{
  "env": {
    "browser": true,
    "es2021": true,
    "node": true
  },
  "extends": ["airbnb-base", "prettier"],
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "rules": {
    "quotes": "warn",
    "no-console": "warn",
    "import/no-extraneous-dependencies": [
      "off",
      {
        "devDependencies": false
      }
    ]
  }
}
